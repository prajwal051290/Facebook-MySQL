<script>
  
  	var home = angular.module('homeAngular', []);
	home.controller('homeController', homeOperations); 
	
	<!--Home Function-->
	
	function homeOperations($scope, $http) {
  	
  	<!-- Fetch User Information-->
  		
  		<!--Get User Information-->
  	
  		$http.get("/getLife")
  			.success(function(data) {
  				console.log("Success returned from Life function");
  				$scope.work = data[0].company_name;
  				$scope.study = data[0].college_name;
  		})  		
  		
  		
  	}
		
</script>


